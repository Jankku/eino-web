{
  "name": "eino-web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "prebuild": "license-checker --production --json --excludePrivatePackages --customPath licenses-format.json --out licenses.json",
    "build": "vite build",
    "preview": "vite preview --host",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "format": "prettier --write ./src",
    "prepare": "husky"
  },
  "dependencies": {
