    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-data-grid": "^8.7.0",
    "@mui/x-date-pickers": "^8.7.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@uidotdev/usehooks": "^2.4.1",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cropperjs": "^2.0.0",
    "diff": "^8.0.2",
